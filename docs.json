{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Retab Docs",
  "logo": {
    "dark": "/logo/retab_bonbon.svg",
    "light": "/logo/retab_bonbon.svg"
  },
  "integrations": {
      "gtm": {
          "tagId": "GTM-K8T2LB6Q"
      }
  },
  "theme": "mint",
  "favicon": "/favicon.png",
  "rounded": "sharp",
  "codeBlock": {
    "mode": "dark"
  },
  "colors": {
    "primary": "#D02572",
    "light": "#F682CB",
    "dark": "#B5367A"
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@Retab.com"
    }
  ],
  "topbarCtaButton": {
    "type": "github",
    "href": "https://github.com/retab-dev/retab"
  },
    "navigation": {
    "global":  {
      "anchors": [
        {
          "anchor": "Site",
          "icon": "globe",
          "href": "https://Retab.dev"
        },
        {
          "anchor": "Github",
          "icon": "github",
          "href": "https://github.com/retab-dev/retab"
        },
        {
          "anchor": "Blog",
          "icon": "book",
          "href": "https://retab.substack.com"
        },
        {
          "anchor": "Community",
          "icon": "discord",
          "href": "https://discord.gg/vc5tWRPqag"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Documentation",
        "icon": "book",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "overview/introduction",
              "overview/quickstart",
              "overview/Build-your-Schema",
              "overview/Best-practices"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "core-concepts/Core_Concepts",
              "core-concepts/Documents",
              "core-concepts/Parsing",
              "core-concepts/Extraction",
              "core-concepts/Parallel_Extraction",
              "core-concepts/Reasoning",
              "core-concepts/k-LLMs_Consensus",
              "core-concepts/Projects",
              "core-concepts/Pricing",
              "core-concepts/Human_In_The_Loop"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "terminal",
        "groups": [
          {
            "group": "API Reference",
            "pages": [
              "api-reference/introduction",
              "api-reference/global/list-models"
            ]
          },
          {
            "group": "Documents",
            "pages": [
              "api-reference/documents/extract",
              "api-reference/documents/parse"
            ]
          },
          {
            "group": "Projects",
            "pages": [
              "api-reference/projects/extract"
            ]
          }
        ]
      }
    ]
